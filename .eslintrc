{
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules":  {
    "prettier/prettier": ["warn", {
      "semi":  true,
      "trailingComma":  "all",
      "singleQuote":  true,
      "printWidth":  80,
      "tabWidth":  2
    }]
  },
  "plugins": [
    "eslint-plugin-prettier"
  ]
}
